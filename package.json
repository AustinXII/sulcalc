{
    "name": "sulcalc",
    "version": "0.3.0",
    "author": "sulcata",
    "private": true,
    "description": "A Pokemon damage calculator.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/sulcata/sulcalc"
    },
    "bugs": {
        "url": "https://github.com/sulcata/sulcalc/issues"
    },
    "scripts": {
        "serve": "webpack-dev-server --config build/webpack.app.dev.js",
        "build": "yarn build:app:production",
        "build:app": "webpack --config build/webpack.app.dev.js",
        "build:app:production": "webpack --config build/webpack.app.prod.js",
        "build:lib": "webpack --config build/webpack.lib.config.js",
        "test": "yarn lint && yarn test:unit",
        "test:unit": "jest --coverage",
        "lint": "eslint '**/*.{js,vue}'",
        "setup": "yarn db && yarn setdex && yarn translations",
        "db": "babel-node scripts/db",
        "setdex": "babel-node scripts/setdex",
        "translations": "babel-node scripts/translations"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.1.2",
        "babel-minify-webpack-plugin": "^0.2.0",
        "babel-plugin-dynamic-import-node": "^1.0.2",
        "babel-plugin-lodash": "^3.2.11",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
        "babel-plugin-transform-export-extensions": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "bootstrap": "4.0.0-beta",
        "css-loader": "^0.28.7",
        "eslint": "^4.6.1",
        "eslint-import-resolver-webpack": "^0.8.3",
        "eslint-plugin-babel": "^4.1.2",
        "eslint-plugin-html": "^3.2.1",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-unicorn": "^2.1.2",
        "extract-text-webpack-plugin": "^3.0.0",
        "fs-extra": "^4.0.1",
        "handlebars": "^4.0.10",
        "handlebars-loader": "^1.6.0",
        "html-webpack-plugin": "^2.30.1",
        "jest": "^21.0.1",
        "lodash": "^4.17.4",
        "script-ext-html-webpack-plugin": "^1.8.5",
        "style-loader": "^0.18.2",
        "vue": "^2.4.2",
        "vue-i18n": "^7.2.0",
        "vue-loader": "^13.0.4",
        "vue-multiselect": "^2.0.2",
        "vue-template-compiler": "^2.4.2",
        "vuex": "^2.4.0",
        "webpack": "^3.5.5",
        "webpack-dev-server": "^2.7.1"
    }
}
